/* pages/album/album.wxss */
.album-wrapper{
  display: flex;
  flex-wrap: wrap;
  justify-content: space-between;
  padding: 40rpx 50rpx;
}
.album-wrapper .album-item{
  width: 300rpx;
  margin-bottom: 20rpx;
}
.album-wrapper .album-item image{
  width: 100%;
  height: 300rpx;
  display: block;
  border-radius: 6rpx;
}
.album-wrapper .album-item text{
  display: block;
  text-align: center;
  font-size: 30rpx;
  margin-top: 10rpx;
}