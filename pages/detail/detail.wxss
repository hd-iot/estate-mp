/* pages/detail/detail.wxss */
.detail-container{
  padding-bottom: 120rpx;
}
.overall-box::after {
  display: block;
  content: ' ';
  height: 24rpx;
  background: #f2f2f2;
  width: 100%;
}

.albumn-box {
  position: relative;
}

.albumn-box .banner {
  width: 100%;
  display: block;
  height: 393rpx;
}

.albumn-box .ico-box {
  display: block;
  width: 66rpx;
  overflow: hidden;
  position: absolute;
  right: 55rpx;
  bottom: 22rpx;
  color: #fff;
  font-size: 26rpx;
  line-height: 37rpx;
}

.albumn-box .ico-box .albumn {
  width: 66rpx;
  height: 66rpx;
  background-size: 66rpx;
  display: block;
  background-image: url(data:image/png;base64,);
}

.albumn-box .ico-box text {
  width: 80rpx;
  letter-spacing: 12rpx;
  display: inline-block;
}

.overall {
  background-color: #fff;
  padding: 33rpx 41rpx 0;
}

.overall .overall-header {
  overflow: hidden;
  margin-bottom: 13rpx;
}

.overall .overall-header .h2 {
  font-size: 44rpx;
  font-weight: bold;
  line-height: 62rpx;
  color: #424242;
  margin-right: 20rpx;
}

.overall .overall-header .ss {
  font-size: 26rpx;
  line-height: 28rpx;
  color: #c5b582;
  margin-top: 24rpx;
}

.overall .tips-box {
  display: flex;
  justify-content: flex-start;
  flex-wrap: wrap;
  margin-bottom: 52rpx;
}

.overall .tips-box .tip {
  padding: 6rpx 17rpx 4rpx;
  line-height: 28rpx;
  background-color: #c5b582;
  color: #fff;
  border-radius: 3px;
  max-width: 124rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 20rpx;
  margin-right: 6rpx;
}

.overall .con {
  font-size: 26rpx;
  line-height: 37rpx;
  padding-bottom: 24rpx;
}

.overall .con .con-item {
  display: flex;
  justify-content: flex-start;
  align-items: flex-start;
  padding: 30rpx 0;
  position: relative;
}

.overall .con .con-item + .con-item {
  border-top: solid 1rpx #e6e6e6;
}

.overall .con .con-item .label {
  flex-basis: 100rpx;
  flex-shrink: 0;
}

.overall .con .con-item .price {
  color: #e74c3c;
}

.overall .con .con-item .price .num {
  font-size: 44rpx;
}

.overall .con .con-item .price .up {
  font-size: 75%;
  position: relative;
  vertical-align: baseline;
  top: -4rpx;
}

.overall .con .con-item .line {
}

.overall .con .con-item .line::before {
  content: '|';
  margin-right: 24rpx;
  margin-left: 24rpx;
}

.overall .con .con-item .br {
  display: block;
}

.foot-box {
  position: fixed;
  left: 0;
  right: 0;
  bottom: 0;
  display: flex;
  align-items: center;
  height: 120rpx;
  text-align: center;
  z-index: 1000;
  background-color: #fff;
  box-shadow: 0 -8rpx 16rpx rgba(0, 0, 0, 0.06);
}

.foot-box .phone {
  flex-grow: 1;
  color: #424242;
  padding-left: 22rpx;
  width: auto;
}

.foot-box .phone .title {
  font-size: 26rpx;
  line-height: 37rpx;
}

.foot-box .phone .num {
  font-size: 36rpx;
  line-height: 50rpx;
  position: relative;
}

.foot-box .phone .num .tel {
  display: inline-block;
  background-image: url(data:image/png;base64,);
  background-size: 48rpx;
  height: 48rpx;
  width: 48rpx;
  left: 16rpx;
  position: absolute;
  background-repeat: no-repeat;
}

.foot-box .book {
  height: 120rpx;
  flex-grow: 1;
  font-size: 36rpx;
  line-height: 120rpx;
  color: #fff;
  background-color: #4a90e2;
}
