/* components/metro/metro.wxss */
@import "/app.wxss";

.tabs-nav-wrap {
  margin-bottom: 17rpx;
  overflow-x: auto;
  overflow-y: hidden;
  border-bottom: 1rpx solid #e6e6e6;
}

.tabs-nav {
  color: #c1c1c1;
  font-size: 36rpx;
  line-height: 50rpx;
  display: flex;
}

.tabs-nav .li {
  text-align: center;
  position: relative;
  width: 147rpx;
  padding-bottom: 20rpx;
  margin-right: 63rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.tabs-nav .li.active {
  color: #4a4a4a;
}

.tabs-nav .li.active::after {
  content: " ";
  display: block;
  width: 100%;
  position: absolute;
  height: 2rpx;
  background-color: #409eff;
  bottom: 0;
  z-index: 111;
}

.tab-content {
  margin-top: 43rpx;
}

.tab-content .view {
  width: 100%;
  overflow: hidden;
  display: block;
  border-radius: 3px;
  margin-bottom: 25rpx;
}

.tra-detail-wrap .item-box {
  position: relative;
  padding: 52rpx 0 60rpx;
  border-bottom: 1rpx solid #f5f5f5;
}

.tra-detail-wrap .item-box .h2 {
  overflow: hidden;
  font-size: 32rpx;
  line-height: 46rpx;
  color: #424242;
  padding-left: 57rpx;
  background-image: url(data:image/png;base64,);
  background-size: 46rpx;
  background-position: 0 center;
  background-repeat: no-repeat;
}

.tra-detail-wrap .item-box .list-box {
  display: flex;
  flex-wrap: wrap;
}

.tra-detail-wrap .item-box .list-box .li {
  width: 120rpx;
  height: 46rpx;
  margin-top: 34rpx;
  line-height: 46rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-size: 26rpx;
  color: #424242;
  border: 1rpx solid #c7c7c7;
  border-radius: 4rpx;
  text-align: center;
  margin-right: 46rpx;
}
.tab-content .swiper-item .view{
  width: 100%;
  border-radius: 3px;
}
.tab-content .self-setting .view{
  width: 100%;
  border-radius: 3px;
}
.tab-content .con,.tab-content .rich-text{
  font-size: 26rpx!important;
}
